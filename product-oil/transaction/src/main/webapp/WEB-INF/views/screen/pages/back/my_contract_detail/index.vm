function confirm(typ,appiontId){

    layui.use(['layer'], function(){
        layer.confirm('Are you sure to confirm it?', {
            title:'Info',
            area: ['535px'],
            type: 1,
            icon: 2,
            //anim: 6, //默认动画风格
            skin: 'demo-class',
            closeBtn: 3,
            shadeClose: false,
            shade: [0.5, '#000000'],
            btnAlign: 'c',
            btn: ['Yes','No'] //按钮
        }, function(){

            confirmBtn(typ,appiontId);

        }, function(){

        });
    });


}

function confirmBtn(typ,appiontId){
    var appointType = typ;
    var appointEnterpriseId = $('#'+appiontId+' option:selected').val();
    var appointEnterpriseName = $('#'+appiontId+' option:selected').text();

    $.ajax({
        type: "POST",
        url: "$appServer/pages/back/my_contract_detail/contractAppoint.json",
        data: {'contractuuid':"$!{contractSheetVO.uuid}",
            'appointType': appointType,
            'appointEnterpriseId': appointEnterpriseId,
            'appointEnterpriseName': appointEnterpriseName
        },
        success: function (msg) {

            layer.alert(msg.message, {title: 'Info', btn: ['OK']}, function () {
                window.location.reload()
            });
        },
        error: function (msg) {

            layer.alert("" + msg.message, {title: 'Info', btn: ['OK']});

        }
    });



}



