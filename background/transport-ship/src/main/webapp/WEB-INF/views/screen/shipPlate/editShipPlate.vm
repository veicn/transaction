  <!-- 内容区域 -->
    <div class="s-mod">
      <div class="s-mod-block">
        <div class="s-mod-btit">船盘信息<span><a href="javascript:;" class="addShip">+ 添加船舶</a></span></div>
        <div class="s-mod-bcont s-mod-bcont4">
          <form class="s-form s-form-cont s-form-cont2" action="">
            <ul>
              <li class="s-form-item">
                <div class="s-inline">
                  <label class="s-form-label"><em>*</em>船名：</label>
                  <div class="s-input-inline">
                    <input type="text" class="s-input width_100 name" name="name" id="name" maxlength="100" >
                  </div>
                </div>
                <div class="s-inline">
                  <label class="s-form-label"><em>*</em>船型：</label>
                  <div class="s-input-inline">
                    <input type="text" class="s-input width_100" id="type" name="type" readonly="true" autocomplete="off" maxlength="100">
                  </div>
                </div>
                <div class="s-inline">
                  <label class="s-form-label">建造年份：</label>
                  <div class="s-input-inline">
                    <input type="text" class="s-input" name="completeDate" id="completeDate" readonly="true" autocomplete="off">
                  </div>
                </div>
                <div class="s-inline">
                  <label class="s-form-label">载重吨：</label>
                  <div class="s-input-inline">
                    <input type="text" class="s-input" id="loadQuantity" name="loadQuantity" readonly="true" autocomplete="off" maxlength="15">
                  </div>
                </div>
                <div class="s-inline">
                  <label class="s-form-label"><em>*</em>船位：</label>
                  <div class="s-input-inline">
                    <input type="text" class="s-input" id="position" name="position" maxlength="100" autocomplete="off">
                  </div>
                </div>
                <div class="s-inline">
                  <label class="s-form-label"><em>*</em>open：</label>
                  <div class="s-input-inline">
                    <input type="text" class="s-input" id="date3" readonly="readonly" name="open" placeholder="请选择日期"/>
                  </div>
                </div>
                <div class="s-inline">
                  <label class="s-form-label"><em>*</em>ETA Fujairah：</label>
                  <div class="s-input-inline">
                    <input type="text" class="s-input" id="date1" readonly="readonly" name="eta" placeholder="请选择日期"/>
                  </div>
                </div>
                <div class="s-inline">
                  <label class="s-form-label">ETA Cabinda：</label>
                  <div class="s-input-inline">
                    <input type="text" class="s-input" id="date2" readonly="readonly" name="etaCabinda" placeholder="请选择日期"/>
                  </div>
                </div>
                <div class="s-inline">
                  <label class="s-form-label"><em>*</em>船东：</label>
                  <div class="s-input-inline">
                    <input type="text" class="s-input owner" name="owner" id="owner" maxlength="100" >
                  </div>
                </div>
                <div class="s-inline">
                  <label class="s-form-label">经纪人：</label>
                  <div class="s-input-inline">
                    <input type="text" class="s-input broker" name="broker" id="broker" maxlength="100" >
                  </div>
                </div>
                <div class="s-inline">
                  <label class="s-form-label">发布日期：</label>
                  <div class="s-input-inline">
                    <input type="text" class="s-input" id="date4" readonly="true" name="publishTime" placeholder="默认"/>
                  </div>
                </div>
                <div class="s-inline">
                  <label class="s-form-label">时效 (天)：</label>
                  <div class="s-input-inline">
                  	<select class="s-select" id="period" name="period">
							<option value="1">1</option>
							<option value="2">2</option>
							<option value="3">3</option>
							<option value="4">4</option>
							<option value="5">5</option>
							<option value="6">6</option>
							<option value="7" selected="true">7</option>
							<option value="8">8</option>
							<option value="9">9</option>
							<option value="10">10</option>
						</select>
                  </div>
                </div>
                <div class="s-inline">
                  <label class="s-form-label">发布人：</label>
                  <div class="s-input-inline">
                    <input type="text" class="s-input" id="epMemberName" readonly="true" name="epMemberName" maxlength="100" autocomplete="off">
                  </div>
                </div>
                <div class="s-inline">
                  <label class="s-form-label">备注：</label>
                  <div class="s-input-inline">
                    <input type="text" class="s-input" id="remark" name="remark" maxlength="100" autocomplete="off">
                  </div>
                </div>
              </li>
            </ul>
          </form>
        </div>
      </div>
      <div class="s-btn-box">
        <button class="s-btn s-btn-blue saveBtn">发&nbsp;&nbsp;布</button>
        <button class="s-btn s-btn-primary returnBtn">返&nbsp;&nbsp;回</button>
      </div>
	  </div>
	  
  	  <script src="../transport/js/shipPlate/editShipPlate.js"></script>
