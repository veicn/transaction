        <div class="s-mod-bcont s-mod-bcont4">
          <form class="s-form s-form-cont s-form-cont2" action="">
            <ul>
              <li class="s-form-item">
                <div class="s-inline">
                  <label class="s-form-label"><em>*</em>IMO：</label>
                  <div class="s-input-inline">
                    <input type="text" class="s-input width_100" name="imo" id="imo" maxlength="15">
                  </div>
                </div>
                <div class="s-inline">
                  <label class="s-form-label">Hull Type：</label>
                  <div class="s-input-inline">
                    <input type="text" class="s-input width_100"  name="hull" id="hull" maxlength="100">
                  </div>
                </div>
                <div class="s-inline">
                  <label class="s-form-label">MMSI：</label>
                  <div class="s-input-inline">
                    <input type="text" class="s-input mmsi" name="mmsi" id="mmsi" placeholder="Please enter the IMO first" maxlength="15">
                  </div>
                </div>
                <div class="s-inline">
                  <label class="s-form-label">SDWT(mt)：</label>
                  <div class="s-input-inline">
                    <input type="text" class="s-input" name="loadQuantity" id="loadQuantity" maxlength="15">
                  </div>
                </div>
                <div class="s-inline">
                  <label class="s-form-label"><em>*</em>Vessel Name：</label>
                  <div class="s-input-inline">
                    <input type="text" class="s-input" name="name" id="name" placeholder="Please enter the ship name" maxlength="100">
                  </div>
                </div>
                <div class="s-inline">
                  <label class="s-form-label">LOA(m)：</label>
                  <div class="s-input-inline">
                    <input type="text" class="s-input" name="length" id="length" maxlength="15" >
                  </div>
                </div>
                <div class="s-inline">
                  <label class="s-form-label">Vessel Type：</label>
                  <div class="s-input-inline">
                    <input type="text" class="s-input" name="useType" id="useType" maxlength="100"/>
                  </div>
                </div>
                <div class="s-inline">
                  <label class="s-form-label">Beam(m)：</label>
                  <div class="s-input-inline">
                    <input type="text" class="s-input"  name="wide" id="wide" maxlength="15"/>
                  </div>
                </div>
                <div class="s-inline">
                  <label class="s-form-label">Vessel Size：</label>
                  <div class="s-input-inline">
                    <input type="text" class="s-input vesselSize" name="type" id="type" maxlength="100">
                  </div>
                </div>
                <div class="s-inline">
                  <label class="s-form-label">Draft(m)：</label>
                  <div class="s-input-inline">
                    <input type="text" class="s-input" name="draft" id="draft" maxlength="15">
                  </div>
                </div>
                <div class="s-inline">
                  <label class="s-form-label"><em>*</em>Built：</label>
                  <div class="s-input-inline">
                    <input type="text" class="s-input " id="date1" name="date1" readonly="true"  placeholder="Please select the building year">
                  </div>
                </div>
                <div class="s-inline">
                  <label class="s-form-label">Cubic(m³)：</label>
                  <div class="s-input-inline">
                    <input type="text" class="s-input" name="capacity" id="capacity" maxlength="15" autocomplete="off">
                  </div>
                </div>
              </li>
              <!-- 附件上传 -->
              <div class="all">
	              <li class="s-form-item wsUl">
	              	<div class="s-inline s-inline100">
