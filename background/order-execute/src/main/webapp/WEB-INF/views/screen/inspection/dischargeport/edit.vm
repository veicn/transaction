<script>
layui.use(['layer', 'ossUpload'], function(){
	$("#submit").on("click",function(){
		$.ajax({
			type:"post",
			url:orderServer+"/inspection/dischargeport/saveDetail.json?id=$!{id}",
			data:$("#inspection_content_form").serialize(),
			success:function(result){
	  			if(result.status != 0){
	  				layer.alert($.i18n(result.code,result.params));
	  			}else{
	  				layer.alert($.i18n("orderexecute.code.00149"),{title:$.i18n("orderexecute.code.00148"),btn:$.i18n("orderexecute.code.00197")}, function(index){
	  					layer.closeAll();
	  					location.reload();
	  				});
	  			}
	  		},
	  		error:function(){
	  			layer.alert("error");
	  		}
		})
	})
	
	var upload = layui.ossUpload;

	//ä¸ä¼ 
	function bindUploader(em){
		upload.render({
		    elem: em,
		    done: function(res, index,upload){
		    	if(res.status === 0){
		    		var hiddenpathId = $(index).attr("data");
		    		
		    		var datajson = {path:res.datas.path,originalName:res.datas.originalName};
		    		$("#"+hiddenpathId).val(JSON.stringify(datajson));
		    		
		    		var em_parent = $("#"+hiddenpathId).parent();
		    		em_parent.find(".s-icon-down").show();
					em_parent.find(".s-icon-view").show();
					em_parent.find(".fs-close").show();
		    	}
		    }
		 })
	}
	//ä¸è½½
	function bindDownloader(){
		$(".s-icon-down").on("click",function(){
			var fileInfoJsonStr = $(this).parent().find("input:hidden").val();
			if(fileInfoJsonStr == ""){
				layer.alert("please upload file",{title:'Prompt',btn:'ok'});
				return;
			}
			
			var fileInfoJson = eval("("+fileInfoJsonStr+")");
			
			window.location.href="$orderExecuteServer.get('/common/doc/download.htm')?path="+ fileInfoJson.path +"&fileName="+ fileInfoJson.originalName;
		})
	}
	//é¢è§
	function bindFileView(){
		$(".s-icon-view").on("click",function(){
			var fileInfoJsonStr = $(this).parent().find("input:hidden").val();
			if(fileInfoJsonStr == ""){
				layer.alert("please upload file",{title:'Prompt',btn:'ok'});
				return;
			}
			var fileInfoJson = eval("("+fileInfoJsonStr+")");
			
			var url = "$orderExecuteServer.get('/common/doc/file.htm')?path="+fileInfoJson.path+"&fileName="+fileInfoJson.originalName;
			window.open("$orderExecuteServer.get('/pdfjs/web/viewer.html?file="+encodeURIComponent(url)+"')","_blank");
		})
	}
	
	//å é¤
	function bindClearFile(){
		$(".fs-close").on("click",function(){
			layer.confirm('Confirm the operation? ', {title:'Prompt',btn:["ok","cancel"]}, function(index){
				$(this).parent().find("input:hidden").val("");
				layer.close(index);
			})
		})
	}
	
	//select init
	$("#voyageSummary_dischargeVolumesSummary_freeWaterBeforeDischarging_method").val("$!{data.voyageSummary_dischargeVolumesSummary_freeWaterBeforeDischarging_method}");
	$("#voyageSummary_dischargeVolumesSummary_shipTotalArrivalVolume_method").val("$!{data.voyageSummary_dischargeVolumesSummary_shipTotalArrivalVolume_method}");
	$("#voyageSummary_dischargeVolumesSummary_shipTotalArrivalVolumeVefApplied_method").val("$!{data.voyageSummary_dischargeVolumesSummary_shipTotalArrivalVolumeVefApplied_method}");
	$("#voyageSummary_dischargeVolumesSummary_shipNetArrivalVolumeVefApplied_method").val("$!{data.voyageSummary_dischargeVolumesSummary_shipNetArrivalVolumeVefApplied_method}");
	$("#quantityResults_method").val("$!{data.quantityResults_method}");
	$("#quantityResults_temp").val("$!{data.quantityResults_temp}");
	$("#qualityTest_sampleLocation").val("$!{data.qualityTest_sampleLocation}");
	
	//ä¸ä¼ ä¸è½½é¢è§å é¤æé®äºä»¶ç»å®
	bindUploader(".fileUpload");
	bindDownloader();
	bindFileView();
	bindClearFile();
	
	$(".dischargeClass").on("click",function(){
		var id = $(this).attr("attr-id");
		var orderNo = $(this).attr("attr-orderNo");		
		layer.confirm('Make sure the page is saved,continue will lose the unsaved infomation', {
            title:'infomation', btn : [ 'Confirm', 'Cancel' ]
        }, function(index) {
            layer.close(index);
            window.location.href="edit.htm?id="+id+"&orderNo="+orderNo;
        }); 
        
		
	})
	
	$(".loadClass").on("click",function(){
		var id = $(this).attr("attr-id");
		var orderNo = $(this).attr("attr-orderNo");
		
		layer.confirm('Make sure the page is saved,continue will lose the unsaved infomation', {
           title:'infomation', btn : [ 'Confirm', 'Cancel' ]
        }, function(index) {
            layer.close(index);
             window.location.href="../loadport/edit.htm?id="+id+"&orderNo="+orderNo;
        }); 
	})
})

function changeSampleLocation(select){
	var selectedOption=select.options[select.selectedIndex];
	if(selectedOption.value == "ship"){
		$("#div_qualityTest_shipTankComposite").show();
		$("#div_qualityTest_shoreTankComposite").hide();
	}else if(selectedOption.value == "shore"){
		$("#div_qualityTest_shoreTankComposite").show();
		$("#div_qualityTest_shipTankComposite").hide();
	}else{
		$("#div_qualityTest_shoreTankComposite").hide();
		$("#div_qualityTest_shipTankComposite").hide();
	}
}

</script>