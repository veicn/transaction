<script>
    var currentPageNum = 1;

    $(function () {
        skiptopage(1);
    })
    /**
     * 上一页/下一页
     * @param num
     */
    function prevnextpage(num) {
        currentPageNum = Number(currentPageNum)+Number(num);
        skiptopage(currentPageNum);
    }

    /**
     * 跳转到指定页
     * @param num
     */
    function skiptopage(num) {
        num = Number(num);
        if (num == 1) {
            $("#prevpage").css("display", "none");
            $("#nextpage").css("display", "");
        } else if (num == 4) {
            $("#prevpage").css("display", "");
            $("#nextpage").css("display", "none");
        } else {
            $("#prevpage").css("display", "");
            $("#nextpage").css("display", "");
        }
        $(".c-cur").removeClass("c-cur");
        $(".c-page").find("a").eq(num).addClass("c-cur");

        if (num == 4) {
            $(".f-rc-infor").each(function (index) {
                if (4*(num-1) <= index) {
                    $(this).css("display", "");
                } else {
                    $(this).css("display", "none");
                }
            })
        } else {
            $(".f-rc-infor").each(function (index) {
                if (4*(num-1) <= index && index < 4*num) {
                    $(this).css("display", "");
                } else {
                    $(this).css("display", "none");
                }
            })
        }
        $(window).scrollTop(0);
    }
</script>
  