#set($layout='')
<!DOCTYPE html>
<html>
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">        
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
	<title>企业详情</title>
	<link rel="stylesheet" href="$imageServer.get('css/s_common.css')">
	<link rel="stylesheet" href="$imageServer.get('css/s_center.css')">
	<script type="text/javascript" src="$appServer.get('script/lib/jquery.validate.min.js')"></script>
	
</head>
<body>
		<div class="c-mr">
			<div class="c-mod" id="cgxx">
			  <h3><em class="cw-ico ico-h3"></em>企业详细信息</h3>
			  <form action="$appServer.get('center/toadd.htm')" method="post" id="from1">
			  <div class="c-form">
				<ul class="c-ful">
					<li class="c-fcos2"><span class="c-fn">企业编号：</span><span class="c-int qx-conte"><input type="text" required="true"  class="c-fcos2" name="code" ></span></li>
					<li class="c-fcos2"><span class="c-fn">企业名称：</span><span class="c-int qx-conte"><input type="text" required="true" class="c-fcos2" name="name" ></span></li>
					<li class="c-fcos2"><span class="c-fn">企业全名：</span><span class="c-int qx-conte"><input type="text" required="true" class="c-fcos2" name="fullName" ></span></li>
					<li class="c-fcos2"><span class="c-fn">组织机构证号：</span><span class="c-int qx-conte"><input type="text" required="true" class="c-fcos2" name="organizationCode" ></span></li>
					<li class="c-fcos2"><span class="c-fn">税号：</span><span class="c-int qx-conte"><input type="text" required="true" class="c-fcos2" name="taxCode" ></span></li>
					<li class="c-fcos2"><span class="c-fn">工商执照证件号：</span><span class="c-int qx-conte"><input type="text" required="true" class="c-fcos2" name="licenseNo" ></span></li>
					<li class="c-fcos2"><span class="c-fn">税务登记证件：</span><span class="c-int qx-conte"><input type="text" required="true" class="c-fcos2" name="taxNo" ></span></li>
					<li class="c-fcos2"><span class="c-fn">创建人：</span><span class="c-int qx-conte"><input type="text" required="true" class="c-fcos2" name="createUser" ></span></li>
					<li class="c-fcos2"><span class="c-fn">描述：</span><span class="c-int qx-conte"><input type="text" required="true" class="c-fcos2" name="description" ></span></li>
				</ul>
				
			  </div>
			  
			<div class="c-subdiv">
				<button type= "submit" class="c-btncas" >提交</button>
			</div>
			  </form>
		    </div>
		    
			
		</div>	
	</div>
	
</body>
