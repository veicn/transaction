<script>

    /*设置reg.code为1*/
    function getUrlParms(name){
        var reg = new RegExp("(^|&)"+ name +"=([^&]*)(&|$)");
        var r = window.location.search.substr(1).match(reg);
        if(r!=null)
            return unescape(r[2]);
        return null;
    }
    var type = getUrlParms("t");
    $(function () {
        type=type||0;
        var regCode = $(".op-regcode");
        regCode.val( type );

        $(".op-tip-"+type).show();

        var a = $(".s-regis-title li");
        a.removeClass("curr");
        a.eq(type).addClass("curr");

        $(".s-tab-regtitle li[data-onshow=user]").hide();
        $(".s-tab-regtitle li[data-onshow=mobile]").hide();
        $(".s-tab-regtitle li[data-onshow=mail]").click();
    });

</script>