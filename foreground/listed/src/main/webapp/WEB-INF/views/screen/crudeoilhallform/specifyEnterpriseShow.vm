
<div class="c-mod c-mod-dd" >
    <h3>
        <em class="cw-ico ico-h3"></em>发布范围
    </h3>
    <div class="c-form c-fbfw">
        <input type="radio"  class="radi ra-fb"  value="0" disabled><em>公开发布</em>
        <input type="radio"  class="radi ra-fb" checked="checked"  value="1" disabled><em>定向发布</em>
    </div>

    <div class="c-fbfw-n">
        <table id="specifyEnterpriseList">
            <tr><th class="tabl-line">序号</th><th>公司名称</th></tr>
            #foreach($specify in $!{specifyEnterpriseList})
                <tr class="c-tabcos2">
                    <td class="tabl-line">$velocityCount</td>
                    <td>$!{specify.enterpriseName}</td>
                </tr>
            #end
        </table>
    </div>

</div>