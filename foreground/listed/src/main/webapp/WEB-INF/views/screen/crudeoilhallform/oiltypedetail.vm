<script>
	var sgRealVal = 0;
	//r1：最小值,r2:最大值,t:input元素
	function checkInputRange(r1,r2,t){
		var tv = t.val();
		if(tv == "" || tv == 0){
            t.val('');
		    return;
		}
		if(tv < r1 || tv > r2){
            layui.use(['layer'], function () {
                layui.layer.alert("所输入的值应该在["+r1+","+r2+"]"+"之间");
            })
			t.val('');
		}
	}
	//apiId 是需要转换的 input, sgId 是 转换目标
	function apiToSG(apiId,sgId){
        sgRealVal = 0; //清空操作
		var apiObj = $('.'+apiId);
		checkInputRange(0,100,apiObj);
		var api = apiObj.val();
		if(api != "" && api != 0){
			var res = division(141.5, add(api,131.5)); //js浮点数丢失精度问题
            sgRealVal = res;
			res = res.toFixed(4);
			$('.'+sgId).val(res);
		}else{
            $('.'+sgId).val('');
		}
	}
	//sgId 是需要转换的 input,  apiId 是 转换目标
	function SGToApi(sgId,apiId){
		var sgObj = $('.'+sgId);
		checkInputRange(0.6112,1.0760,sgObj);
		var sg = sgObj.val();
		if(sg != "" && sg != 0){
			var res = subduction(division(141.5, sgRealVal == 0 ?sg:sgRealVal.toFixed(5)),131.5);//js浮点数丢失精度问题
            sgRealVal = 0;
			res = Number(res).toFixed(2);
			$('.'+apiId).val(res);
		}else{
            $('.'+apiId).val('');
		}
	}
	
