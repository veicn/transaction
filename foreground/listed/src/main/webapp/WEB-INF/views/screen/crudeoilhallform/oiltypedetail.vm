                var index = inputHtml.substr(inputHtml.indexOf('[')+1,1)-1;
                next.find('input').each(function (i,v) {
                    var nameVal = $(v).attr('name');
                    if(nameVal != undefined && nameVal != ''){
                        nameVal = 'crudeOilFormList['+index+'].'+nameVal.substr(nameVal.indexOf('.')+1);
                        $(v).attr('name',nameVal);
                    }
                })
                
                next.find('.crudeOilLayer').attr('data-index', index+1);

            });
            trData.remove();
